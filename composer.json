{
  "name": "talent-loom/laravel-quickbooks-v3",
  "description": "Talent Loom's Laravel for QuickBooks.",
  "keywords": [
    "quickbooks",
    "client",
    "laravel",
    "talent-loom"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Syed Shazeedul Islam",
      "email": "syedshazeedul@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/database": "^10.48",
    "illuminate/http": "^10.48",
    "illuminate/routing": "^10.48",
    "illuminate/support": "^10.48",
    "nesbot/carbon": "^2.72",
    "quickbooks/v3-php-sdk": "^6.1"
  },
  "require-dev": {
    "laravel/pint": "^1.17",
    "mockery/mockery": "^1.6",
    "phpunit/phpunit": "^10.5",
    "psy/psysh": "^0.12.4",
    "scrutinizer/ocular": "^1.9"
  },
  "autoload": {
    "psr-4": {
      "TalentLoom\\LaravelQuickbooksV3\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TalentLoom\\LaravelQuickbooksV3\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TalentLoom\\LaravelQuickbooksV3\\Providers\\ServiceProvider",
        "TalentLoom\\LaravelQuickbooksV3\\Providers\\ClientServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}